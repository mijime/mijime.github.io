parser: babel-eslint
env:
  es6: true
  node: true
rules:
  block-scoped-var: 2
  brace-style:
  - 1
  - 1tbs
  - allowSingleLine: true
  comma-spacing:
  - 1
  - after: true
    before: false
  comma-style:
  - 2
  - last
  complexity:
  - 1
  - 3
  consistent-this:
  - 2
  - _this
  curly:
  - 2
  - all
  default-case: 2
  dot-notation: 1
  func-names: 1
  func-style:
  - 2
  - declaration
  guard-for-in: 1
  indent:
  - 2
  - 2
  key-spacing:
  - 1
  - afterColon: true
    align: value
    beforeColon: false
  max-len:
  - 1
  - 100
  - 2
  max-nested-callbacks:
  - 1
  - 3
  max-params:
  - 1
  - 4
  max-statements:
  - 1
  - 12
  new-cap:
  - 1
  - capIsNew: true
    newIsCap: true
  no-alert: 0
  no-catch-shadow: 2
  no-console: 1
  no-constant-condition: 1
  no-debugger: 1
  no-div-regex: 2
  no-else-return: 2
  no-eq-null: 2
  no-extend-native: 1
  no-extra-bind: 0
  no-extra-boolean-cast: 0
  no-extra-parens: 1
  no-floating-decimal: 2
  no-iterator: 1
  no-lonely-if: 1
  no-loop-func: 1
  no-multi-spaces:
  - 2
  - exceptions:
      AssignmentExpression: true
      ImportDeclaration: true
      Property: true
      VariableDeclarator: true
  no-multi-str: 1
  no-multiple-empty-lines:
  - 1
  - max: 2
  no-nested-ternary: 2
  no-path-concat: 1
  no-process-env: 1
  no-proto: 1
  quote-props: 2
  no-self-compare: 2
  no-sync: 1
  no-throw-literal: 1
  no-trailing-spaces: 1
  no-undef-init: 1
  no-undefined: 1
  no-underscore-dangle: 0
  no-unused-expressions: 1
  no-unused-vars:
  - 1
  - args: after-used
    vars: all
  no-var: 1
  operator-linebreak:
  - 1
  - before
  padded-blocks:
  - 2
  - never
  quotes:
  - 2
  - single
  radix: 1
  semi:
  - 1
  semi-spacing:
  - 1
  - after: true
    before: false
  space-before-blocks: 2
  space-before-function-paren:
  - 2
  - never
  array-bracket-spacing:
  - 2
  - never
  space-in-parens:
  - 2
  - never
  space-infix-ops: 1
  keyword-spacing: 2
  space-unary-ops:
  - 2
  - nonwords: false
    words: true
  strict: 2
  wrap-iife:
  - 2
  - inside
  yoda: 0
