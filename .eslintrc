env:
  node: true
parser: babel-eslint
rules:
  brace-style:
  - 2
  - 1tbs
  - allowSingleLine: false
  comma-style:
  - 2
  - last
  consistent-this:
  - 2
  - DO NOT USE
  default-case: 2
  indent:
  - 1
  - 2
  key-spacing:
  - 2
  - afterColon: true
    align: colon
    beforeColon: true
  max-depth:
  - 1
  - 3
  max-len:
  - 1
  - 100
  - 2
  max-params:
  - 1
  - 5
  no-else-return: 2
  no-lonely-if: 2
  no-multi-spaces:
  - 2
  - exceptions:
      AssignmentExpression: true
      ImportDeclaration: true
      Property: true
      VariableDeclarator: true
  no-multiple-empty-lines:
  - 2
  - max: 1
  no-nested-ternary: 2
  no-new-require: 2
  no-self-compare: 2
  no-throw-literal: 2
  no-unused-expressions: 0
  no-use-before-define: 0
  no-var: 2
  no-void: 2
  quotes:
  - 2
  - single
  space-before-blocks:
  - 2
  - always
  strict: 2
